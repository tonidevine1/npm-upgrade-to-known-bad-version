      "jwt-decode": "^2.2.0",
      "promise-retry": "^1.1.1",
      "request": "^2.88.2",
      "request-promise": "^4.2.6",
      "secure-random-password": "^0.2.1",
      "uuid": "^7.0.3"
    },
    "devDependencies": {
      "@conqa/serverless-openapi-documentation": "^1.1.0",
      "aws-sdk": "^2.780.0",
      "aws-sdk-mock": "^4.0.0",
      "babel-eslint": "^10.1.0",
      "chai": "^4.1.2",
      "chai-as-promised": "^7.1.1",
      "chai-string": "^1.4.0",
      "eslint": "^5.16.0",
      "eslint-config-prettier": "^4.1.0",
      "eslint-plugin-prettier": "^3.1.4",
      "jsdoc": "^3.6.6",
      "jwt-simple": "^0.5.6",
      "lambda-tester": "^4.0.1",
      "mocha": "^6.2.3",
      "nyc": "^14.1.1",
      "pre-commit": "1.2.2",
      "prettier": "^1.19.1",
      "s3rver": "^3.6.1",
      "semver-regex": "^4.0.1",
      "serverless": "^2.59.0",
      "serverless-domain-manager": "^2.6.5",
      "serverless-dynamodb-local": "^0.2.39",
      "serverless-offline": "^9.0.0",
      "serverless-offline-sns": "^0.76.0",
      "serverless-offline-ssm": "^5.1.0",
      "serverless-plugin-bind-deployment-id": "^1.2.0",
      "serverless-plugin-git-variables": "^3.5.0",
      "serverless-plugin-stage-variables": "^1.9.4",
      "serverless-s3-local": "^0.6.7",
      "sinon": "^7.3.2",
      "sinon-chai": "^3.5.0",
      "sinon-test": "^2.4.3",
      "snyk": "^1.420.2",
      "spawn-sync": "2.0.0",
      "try-thread-sleep": "2.0.0"
    },
    "scripts": {
      "synk": "snyk test",
      "install:lambda": "docker pull lambci/lambda",
      "install:dynamodb": "ENVIRONMENT=local ./node_modules/.bin/sls dynamodb install --stage local",
      "start:local": "ENVIRONMENT=local ./node_modules/.bin/sls offline start -r us-east-1 --noTimeout --stage local",
      "start:debug": "ENVIRONMENT=local node --inspect ./node_modules/.bin/sls offline start -r us-east-1 --noTimeout --stage local",
      "git-secrets": "echo '[git-secrets]: running scan.'; ./tools/awslabs/git-secrets.sh --add-provider -- cat ./.gitsecrets; git ls-files | grep -v -f ./.gitsecretsignore | xargs -P15 ./tools/awslabs/git-secrets.sh --scan",
      "test": "snyk test &&  nyc -a mocha --exit 'src/test/**/*.test.js'",
      "test:integration": "mocha --exit 'src/integration_tests/**/*.test.js'",
      "test:ci": "nyc --all --reporter=cobertura --reporter=lcov --report-dir reports/coverage/ mocha --exit './src/test/**/*.test.js' && npm run test:coverage",
      "test:debug": "mocha --inspect-brk",
      "test:coverage": "nyc check-coverage --statements 60 --lines 60 --functions 59 --branches 60",
      "lint": "eslint src/*.js",
      "lint:ci": "eslint --config .eslintrc.js --ignore-path .eslintignore --format checkstyle --output-file  reports/style/eslint.xml src/*.js",
      "centralScenarioGeneration": "node -e 'require(\"./src/mocker/central-generation\").generate();'",
      "databaseMigration": "node -e 'require(\"./src/migration/migration\").startMigrations();'",
      "createTestDatasetsForFtdTest": "node -e 'require(\"./src/mocker/test-datasets-for-scenarios\").create();'",
      "snyk-protect": "snyk protect",
      "prepare": "npm run snyk-protect",
      "jsdoc": "npx jsdoc -c ./jsdoc.json --readme ./JSDOC.md"
    },
    "pre-commit": [
      "git-secrets"
    ],
    "nyc": {
      "exclude": [
        "config",
        "src/integration_tests",
        "**/*.test.js",
        "reports",
        ".eslintrc.js",
        ".prettierrc.js",
        "envconfig.js",
        "**/*-test.js",
        "src/test",
        "docs/jsdocs/"
      ]
    }
  }